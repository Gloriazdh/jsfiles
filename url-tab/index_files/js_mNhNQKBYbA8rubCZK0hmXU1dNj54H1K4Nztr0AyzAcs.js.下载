            function safari_width() {
                var ul_width = "";
                if($('body').width() < 991) {
                    ul_width = $('.page-find-your-programme .main-container section.col-sm-9 .region-content .find-your-programme-final-wrapper').width();
                    $('.page-find-your-programme .find-your-programme-final-wrapper li').css({
                        'width' : ul_width / 12
                    });
                } else {
                    ul_width = $('.page-find-your-programme .main-container section.col-sm-9 .region-content .find-your-programme-final-wrapper').width();
                    $('.page-find-your-programme .find-your-programme-final-wrapper li').css({
                        'width' : (ul_width - 300) / 12
                    });
                }
            }

            if (navigator.userAgent.indexOf('Safari') != -1 && navigator.userAgent.indexOf('Chrome') == -1) {
                safari_width();
            }

            function container_tab() {
                var cont = '.intro-block .region-intro .flexslider .slides li > div .container';
                var container_sirina = $(cont).css('width');
                var container_sirina2 = $(cont).outerWidth(true);
            }

            container_tab();

            function faculty_profiles_slider() {
                setTimeout(function(){
                   var ul = ".page-faculty-profiles .main-container .row .col-sm-9 ul.tabs--primary";
                    var wrapper = ".faculty-profiles-wrapper";

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $( ".page-faculty-profiles .main-container .row .col-sm-9 ul.tabs--primary li").each(function(index) {
                        var current_width = parseInt($(this).width(), 10);
                        if (current_width > 0) {
                            totalWidth += current_width;
                        }
                    });


                    totalWidth += 50;

                    // ul offset //

                    var ul_offset = $(ul).offset();

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(wrapper).append('<div class="previous_slider"></div>');
                        $(wrapper).append('<div class="next_slider"></div>');

                        var color = 'rgb(191, 0, 0)';
                        function rgb2hex(rgb){
                            rgb = rgb.match(/^rgba?[\s+]?\([\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?/i);
                            return (rgb && rgb.length === 4) ? "#" +
                            ("0" + parseInt(rgb[1],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[2],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[3],10).toString(16)).slice(-2) : '';
                        }

                        var color_hex = rgb2hex(color);

                        function ColorLuminance(hex, lum) {
//
                            // validate hex string
                            hex = String(hex).replace(/[^0-9a-f]/gi, '');
                            if (hex.length < 6) {
                                hex = hex[0]+hex[0]+hex[1]+hex[1]+hex[2]+hex[2];
                            }
                            lum = lum || 0;
//
                            // convert to decimal and change luminosity
                            var rgb = "#", c, i;
                            for (i = 0; i < 3; i++) {
                                c = parseInt(hex.substr(i*2,2), 16);
                                c = Math.round(Math.min(Math.max(0, c + (c * lum)), 255)).toString(16);
                                rgb += ("00"+c).substr(c.length);
                            }
//
                            return rgb;
                        }

                        var darker_color_hex = ColorLuminance(color_hex, -0.1);

                        $('.next_slider').css({
                            'background-color' : darker_color_hex
                        });
                        $('.previous_slider').css({
                            'background-color' : darker_color_hex
                        });

                        $(ul).addClass('ul_margin');



                        $('.next_slider').click(function () {
                            var ul_offset_next = $(ul).offset();

                            if ((ul_offset_next.left + width_differences - ul_offset.left) < 150) {
                                $(ul).css({
                                    'left' : -1 * (width_differences) - 50
                                })
                            } else {
                                $(ul).css({
                                    'left': '-=150px'
                                });
                            }

                            var ul_css_left_temp = $(ul).css('left');
                            var ul_css_left = ul_css_left_temp.substr(0, ul_css_left_temp.length-2);
                        });

                        $('.previous_slider').click(function () {
                            $(ul).css({
                                'left': 0
                            });
                        });
                    }

                }, 500);
            }

            function alphabet_slider() {
                    $( ".page-faculty-profiles .view-faculty-profiles .attachment ul.views-summary li").each(function(index) {
                        var child = $(this).children('a');
                        $(this).html(child);
                    });


                setTimeout(function(){



                    var ul = ".page-faculty-profiles .view-faculty-profiles .attachment ul.views-summary";
                    var wrapper = ".faculty-profiles-wrapper-alphabet";

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $( ".page-faculty-profiles .view-faculty-profiles .attachment ul.views-summary li").each(function(index) {
                        var current_width = parseInt($(this).width(), 10);
                        if (current_width > 0) {
                            totalWidth += current_width;
                        }
                    });

                    // ul offset //

                    var ul_offset = $(ul).offset();

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_alphabet').remove();
                        $('.next_alphabet').remove();
                        $(wrapper).append('<div class="previous_alphabet"></div>');
                        $(wrapper).append('<div class="next_alphabet"></div>');

                        $('.next_alphabet').css({
                            'color' : "#BF0000"
                        });
                        $('.previous_alphabet').css({
                            'color' : "#BF0000"
                        });

                        $(ul).css({
                            'margin-left' : '20px'

                        });


                        $('.next_alphabet').click(function () {
                            var ul_offset_next = $(ul).offset();

                            if ((ul_offset_next.left + width_differences - ul_offset.left) < 55) {
                                $(ul).css({
                                    'left' : -1 * (width_differences) - 40
                                });
                            } else {
                                $(ul).css({
                                    'left': '-=55px'
                                });
                            }

                            var ul_css_left_temp = $(ul).css('left');
                            var ul_css_left = ul_css_left_temp.substr(0, ul_css_left_temp.length-2);
                        });

                        $('.previous_alphabet').click(function () {
                            $(ul).css({
                                'left': 0
                            });

                        });
                    }
                }, 500);
            }

            function Body() {
                setTimeout(function(){
                    var ul = ".main-container section.col-sm-9 .region-content .fc-tabs-formatter ul.nav-tabs";
                    var wrapper = ".fc-tabs-formatter";

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $( ".main-container section.col-sm-9 .region-content .fc-tabs-formatter ul.nav-tabs li").each(function(index) {
                        var current_width = parseInt($(this).width(), 10);
                        if (current_width > 0) {
                            totalWidth += current_width;
                        }
                    });

                    totalWidth += 50;

                    // ul offset //

                    var ul_offset = $(ul).offset();

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(wrapper).append('<div class="previous_slider"></div>');
                        $(wrapper).append('<div class="next_slider"></div>');



                        var color = $(".main-container section.col-sm-9 .region-content .fc-tabs-formatter ul.nav-tabs li:last-child a").css('background-color');

                        if (color.length == 7) {
                            color_hex = color;
                        } else {
                            function rgb2hex(rgb) {
                                rgb = rgb.match(/^rgba?[\s+]?\([\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?/i);
                                return (rgb && rgb.length === 4) ? "#" +
                                ("0" + parseInt(rgb[1], 10).toString(16)).slice(-2) +
                                ("0" + parseInt(rgb[2], 10).toString(16)).slice(-2) +
                                ("0" + parseInt(rgb[3], 10).toString(16)).slice(-2) : '';
                            }

                            var color_hex = rgb2hex(color);
                        }
                        function ColorLuminance(hex, lum) {
//
                            // validate hex string
                            hex = String(hex).replace(/[^0-9a-f]/gi, '');
                            if (hex.length < 6) {
                                hex = hex[0]+hex[0]+hex[1]+hex[1]+hex[2]+hex[2];
                            }
                            lum = lum || 0;
//
                            // convert to decimal and change luminosity
                            var rgb = "#", c, i;

                            for (i = 0; i < 3; i++) {
                                c = parseInt(hex.substr(i*2,2), 16);
                                c = Math.round(Math.min(Math.max(0, c + (c * lum)), 255)).toString(16);
                                rgb += ("00"+c).substr(c.length);
                            }
//
                            return rgb;
                        }

                        var darker_color_hex = ColorLuminance(color_hex, -0.1);

                        $('.next_slider').css({
                            'background' : darker_color_hex
                        });
                        $('.previous_slider').css({
                            'background' : darker_color_hex
                        });

                        $(ul).addClass('ul_margin');



                        $('.next_slider').click(function () {
                            var ul_offset_next = $(ul).offset();

                            if ((ul_offset_next.left + width_differences - ul_offset.left) < 150) {
                                $(ul).css({
                                    'left' : -1 * (width_differences) - 50
                                });
                            } else {
                                $(ul).css({
                                    'left': '-=150px'
                                });
                            }

                            var ul_css_left_temp = $(ul).css('left');
                            var ul_css_left = ul_css_left_temp.substr(0, ul_css_left_temp.length-2);

                        });

                        $('.previous_slider').click(function () {

                                $(ul).css({
                                    'left': 0
                                });
                        });
                    } else {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(ul).removeClass('ul_margin');
                        $(ul).css({
                            'left' : '0'
                        });
                    }

                }, 500);
            }
			 
			 //this is the active function
             function Body() {
                setTimeout(function(){
                    var ul = ".main-container section .region-content .fc-tabs-formatter ul.nav-tabs";
                    var wrapper = ".fc-tabs-formatter";

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $( ".main-container section .region-content .fc-tabs-formatter ul.nav-tabs li").each(function(index) {
                        var current_width = parseInt($(this).width(), 10);
                        if (current_width > 0) {
                            totalWidth += current_width;
                        }
                    });

                    totalWidth += 50;

                    // ul offset //

                    var ul_offset = $(ul).offset();

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
