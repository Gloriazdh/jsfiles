            });

            var next_sliderrrr = 2;

            $('#block-ceibs-intro-hp-programs-block ul.nav-tabs li').click(function(){
               next_sliderrrr = parseInt($(this).attr('data-slide-to')) + 1;
            });


            window.setInterval(function(){
                $('#block-ceibs-intro-hp-programs-block .tab-content .tab-pane').removeClass('active');
                $('#block-ceibs-intro-hp-programs-block .tab-content .a' + next_sliderrrr).addClass('active');

                $('#block-ceibs-intro-hp-programs-block ul.nav-tabs li').removeClass('active');
                $('#block-ceibs-intro-hp-programs-block ul.nav-tabs li.a' + next_sliderrrr).addClass('active');

                if(next_sliderrrr == cntttt-1) {
                    next_sliderrrr = 1;
                }
                else {
                    next_sliderrrr++;
                }
            }, 8000);
			
			
            // homepage menu fix //
            var div = document.createElement("div");
            div.innerHTML = "<!--[if lt IE 9]><i></i><![endif]-->";
            var isIeLessThan9 = (div.getElementsByTagName("i").length == 1);
            if (!isIeLessThan9) {
                $('#navbar nav .menu > li').hover(function() {
                    if ($(this).children().length > 1) {
                        var ddm = $(this).find('> .dropdown-menu');
                        var dropdown_offset = ddm.offset();
                        var right_offset = ($(window).width() - dropdown_offset.left - ddm.outerWidth());

                        if (right_offset < 0) {
                            ddm.css({
                                'left': '-100%',
                                'width': '294px'
                            });
                        }
                    }
                }, function() {
                });

                $('#navbar nav .menu li .dropdown-menu > li').hover(function() {
                    if ($(this).children().length > 1) {
                        var ddm = $(this).find('> .dropdown-menu');
                        var dropdown_offset = ddm.offset();
                        var right_offset = ($(window).width() - dropdown_offset.left - ddm.outerWidth());

                        if (right_offset < 0) {
                            ddm.css({
                                'left': '-294px',
                                'width': '294px',
                                'padding': '27px'
                            });
                        }
                    }
                }, function() {
                });


                $('#navbar nav .menu li .dropdown-menu li .dropdown-menu > li').hover(function() {
                    if ($(this).children().length > 1) {
                        var ddm = $(this).find('> .dropdown-menu');
                        var dropdown_offset = ddm.offset();
                        var right_offset2 = ($(window).width() - dropdown_offset.left - (2 * ddm.outerWidth()));

                        if (right_offset2 < 0) {
                            ddm.css({
                                'left': '294px',
                                'width': '294px'
                            });
                        }
                    }
                }, function() {
                });

