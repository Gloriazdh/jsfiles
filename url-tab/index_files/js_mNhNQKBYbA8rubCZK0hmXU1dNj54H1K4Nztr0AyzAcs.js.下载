            $(categoriesWithContentMultiple).each(function(i,v){
                var categoriesMultiple = v;
                
                var keyName = Object.keys(categoriesMultiple)[0];
                var splittedKey = keyName.split('_');

                $(splittedKey).each(function(j,val){
      
                    var finalContent = categoriesMultiple[keyName];
                    var elem = $("h3."+val);

                    var categoryColor = elem.css('color');
                    $(finalContent).find('a').css({"color":categoryColor});
                    
                    var sticky = $(finalContent).find('.sticky');
                    sticky.css({"background-color":categoryColor});

                    if(sticky.find('span').html() != ""){
                        sticky.show();
                    } else {
                         sticky.hide();
                    }

                    $(finalContent).find('.views-field-field-oep-category').remove();
                    
                    $(finalContent).clone().insertAfter(elem.next());
                   
                })

            })       

