					if($(".vjs-tech", this).length>0) {
                      $(".vjs-tech", this).get(0).play();
                    }
                }
            });
            // all video with front image
            $(".group-video-holder").click(function(){
                if (!$(this).hasClass('none-image')) {
                    $(this).addClass('none-image');
                    $("iframe", this)[0].src += "&autoplay=1";
                }
            });
            // sidebar and content videos
            $(".video-wrapper").click(function(){

                if($(this).find('a').length == 0) {
                    if (!$(this).hasClass('none-image')) {
                        $(this).addClass('none-image');

						var div_id = $(this).attr('id');
						var video_id_youtube = '#'+div_id+' .youtube-field-player';

						if($(video_id_youtube).length>0) {
						  $(video_id_youtube)[0].src += "&autoplay=1";
						}

						var video_id_html5 = '#'+div_id+' .video-js .vjs-tech';
						if($(video_id_html5).length>0) {
						  $(video_id_html5).play();
						}

						var video_id_direct = '#'+div_id+' .direct-video';
						if($(video_id_direct).length>0) {
						  $(video_id_direct).get(0).play();
						}

						var video_id_embed = '#'+div_id+' embed';
						if($(video_id_embed).length>0) {
						  $(video_id_embed).get(0).Play();
						  //$(video_id_embed).Play();
						}

						if($(".vjs-tech", this).length>0) {
						  $(".vjs-tech", this).get(0).play();
						}
                    }
                }
                else {
                    //alert("sdf");
                    //$(this).find('a').click();
                    //$(this).find('a').trigger( "click" );
                }
            });
            // alumni video
            $(".view-id-featured_video").click(function(){
