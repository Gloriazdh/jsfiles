jQuery(function($){
  var $context = $('.page-alumni-club');
  $('.tabs--primary',$context).find('a').each(function(i){
     $(this).attr('href', $(this).attr('href') + '#main-content');s
  }); 
  
  var $context = $('.quick-login');
  var timeSetting;
  var timeOutAction = function(){
        $('.quick-login-items', $context).hide();
      };

  $('.quick-login-text a', $context).hover(function(e){
     clearTimeout(timeSetting);
     $('.quick-login-items', $context).show();
  }, function(e){
      timeSetting = setTimeout(timeOutAction, 600);
  });
  $('.quick-login-items', $context).hover(function(e){
      clearTimeout(timeSetting);
  },function(e){
     clearTimeout(timeSetting);
     timeSetting = setTimeout(timeOutAction, 600);

  });

});

;
