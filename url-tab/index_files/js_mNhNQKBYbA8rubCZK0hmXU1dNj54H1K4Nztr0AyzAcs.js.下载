            $(".page-find-your-programme .find-your-programme-final-wrapper").before("<div class='expand-all'>" + Drupal.t('EXPAND ALL') + "</div>");
            var expand_click = 0;
            $('.expand-all').click(function() {
                $('.oep_months').removeClass('programme-height');
                $('.program-info').removeClass('programme-height');

                if((expand_click % 2) === 0 ) {
                    $('.page-find-your-programme .find-your-programme-final-wrapper .program-title').each(function () {
                        var info_height = 50;
                        $(this).parent().children().each(function () {
                            info_height += $(this).outerHeight(true);
                        });

                        $(this).parent().css({
                            'height': info_height
                        });

                        $(this).parent().next().css({
                            'height': info_height
                        });
                    });

                    $(".page-find-your-programme .program-title").find('.span_plus').html('-');
                } else {
                    $('.page-find-your-programme .find-your-programme-final-wrapper .program-title').each(function () {
                        var height = $(this).height();

                        $(this).parent().css({
                            'height': height + 50
                        });

                        $(this).parent().next().css({
                            'height': height + 50
                        });
                    });

                    $(".page-find-your-programme .program-title").find('.span_plus').html('+');
                }

                expand_click++;
            });

            $(".page-find-your-programme .program-title").append("<div class='span_plus'>+</div>");

            $('ul.oep_months li span').append('<br>');
            var even_odd = 0;
            var previousTarget = null;
            $('.program-info-wrapper').click(function(){
                var info_height = 50;
                $(this).find('.program-info').children().each(function(){
                   info_height += $(this).outerHeight(true);
                });


                $("<style type='text/css'> .programme-height { height: "+info_height+"px !important; } </style>").appendTo("head");


                if(this === previousTarget) {
                    even_odd++;
                    if((even_odd % 2) === 0) {
                        $(".page-find-your-programme .program-title").find('.span_plus').html('+');
                        $(this).find('.span_plus').html('-');

                        $(this).find('.oep_months').toggleClass('programme-height');
                        $(this).find('.program-info').toggleClass('programme-height');

                    } else {
                        $(".page-find-your-programme .program-title").find('.span_plus').html('+');
                        $(this).find('.span_plus').html('+');

                        $(this).find('.oep_months').toggleClass('programme-height');
                        $(this).find('.program-info').toggleClass('programme-height');
                    }

                } else {
                    even_odd = 0;
                    previousTarget = this;

                    $(".page-find-your-programme .program-title").find('.span_plus').html('+');
                    $(this).find('.span_plus').html('-');

                    $('.oep_months').removeClass('programme-height');
                    $('.program-info').removeClass('programme-height');

                    $(this).find('.oep_months').toggleClass('programme-height');
                    $(this).find('.program-info').toggleClass('programme-height');
                }
            });

            $('#edit-title').attr("placeholder", Drupal.t("Search by name, interests or other keywords"));
            $('#edit-title').css({
                'width' : '340px'
            });

            // find your programme slider //
            function buisness_slider() {
                var fyp_border = $('.page-find-your-programme .main-container .row form .form-item-business-size .selectricWrapper .selectricItems');
                $('.buisness_slider').remove();
                fyp_border.append('<div class="buisness_slider"></div>');
                fyp_border.append('<div class="buisness_slider_white"></div>');
                var counter_li = 1;
                $('.page-find-your-programme .main-container .row form .form-item-business-size .selectricWrapper .selectricItems ul li').each(function() {
                    $(this).addClass("number" + counter_li);

                    if($(this).hasClass('selected').toString() == "true") {
                        var class_names = $(this).attr('class');
                        var number_in_li = class_names.match(/\d+/)[0];

                        $('.buisness_slider').css({
                            'height' : number_in_li * 30 - 17
                        });
                    };
                    counter_li++;
                    $('.buisness_slider_white').css({
                        'height' : counter_li * 30 -47
                    });
                });
            }

            buisness_slider();

            $('.page-find-your-programme .main-container .row form .form-item-business-size .selectricWrapper .selectricItems ul li').click(function() {
                buisness_slider();
            });

            // end //

