            $('.alumni-maps-switch').click(function(){
                var fontawesome_i = $('.page-chapters .col-sm-9 .region #block-system-main .alumni-maps-switch i');
                if(fontawesome_i.hasClass('fa-angle-down').toString() == "true") {
                    fontawesome_i.removeClass('fa-angle-down');
                    fontawesome_i.addClass('fa-angle-up');
                } else {
                    fontawesome_i.removeClass('fa-angle-up');
                    fontawesome_i.addClass('fa-angle-down');
                }
                $('.regions-list').toggleClass('region-list-show');

