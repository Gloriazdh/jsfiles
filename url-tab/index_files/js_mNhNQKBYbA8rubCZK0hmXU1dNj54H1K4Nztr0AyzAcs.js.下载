            // video rezize//

            function video_resize () {
                setTimeout(function(){
                    $('.hp-video-wrapper').each(function(){
                        var video_width = Math.round($(this).width());
                        var video_height = Math.round(video_width / 1.66) + 10;
                        $(this).attr('height', video_height);
                        $(this).css({
                            'height' : video_height
                        });

                        $(this).find('.video-js').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                        $(this).find('.youtube-field-player').css({
                            'width' : video_width,
                            'height' : video_height
                        });

                        $(this).find('img').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                    });

                    $('.video-wrapper').each(function(){
                        var video_width = Math.round($(this).width());
                        var video_height = Math.round(video_width / 1.66) + 10;
                        $(this).attr('height', video_height);
                        $(this).css({
                            'height' : video_height
                        });

                        $(this).find('.video-js').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                        $(this).find('.youtube-field-player').css({
                            'width' : video_width,
                            'height' : video_height
                        });

                        $(this).find('img').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                    });

                    $('.group-video-holder').each(function(){
                        var video_width = Math.round($(this).width());
                        var video_height = Math.round(video_width / 1.3333333333) + 10;
                        $(this).css({
                            'height' : video_height
                        });

                        $(this).find('.video-js').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                        $(this).find('.youtube-field-player').css({
                            'width' : video_width,
                            'height' : video_height
                        });

                        $(this).find('img').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                    });

                    $('.group-video-wrapper').each(function(){
                        var video_width = Math.round($(this).width());
                        var video_height = Math.round(video_width / 1.3333333333) + 10;
                        $(this).css({
                            'height' : video_height
                        });

                        $(this).find('.video-js').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                        $(this).find('.youtube-field-player').css({
                            'width' : video_width,
                            'height' : video_height
                        });

                        $(this).find('img').css({
                            'width' : video_width,
                            'height' : video_height
                        });
                    });
                }, 500);
            };

            video_resize();

