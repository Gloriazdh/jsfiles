            function years_scroll() {
                setTimeout(function(){
                    var ul = ".page-alumni-magazine-archive .main-container section.col-sm-9 .region-content .selectricScroll ul";
                    var wrapper = ".faculty-profiles-wrapper";

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $( ".page-alumni-magazine-archive .main-container section.col-sm-9 .region-content .selectricScroll ul li").each(function(index) {
                        totalWidth += 100;
                    });
					
                    $(ul).css({
                        'width' : totalWidth
                    });

                    totalWidth += 50;

                    // ul offset //

                    var ul_offset = $(ul).offset();

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(wrapper).append('<div class="previous_slider"></div>');
                        $(wrapper).append('<div class="next_slider"></div>');

                        var color = '#C80000'; //$(".page-alumni-magazine-archive .main-container section.col-sm-9 .region-content .selectricScroll ul li.last").css('background-color');

                        function rgb2hex(rgb){
                            rgb = rgb.match(/^rgba?[\s+]?\([\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?/i);
                            return (rgb && rgb.length === 4) ? "#" +
                            ("0" + parseInt(rgb[1],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[2],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[3],10).toString(16)).slice(-2) : '';
                        }

                        var color_hex = rgb2hex(color);

                        function ColorLuminance(hex, lum) {
//
                            // validate hex string
                            hex = String(hex).replace(/[^0-9a-f]/gi, '');
                            if (hex.length < 6) {
                                hex = hex[0]+hex[0]+hex[1]+hex[1]+hex[2]+hex[2];
                            }
                            lum = lum || 0;
//
                            // convert to decimal and change luminosity
                            var rgb = "#", c, i;
                            for (i = 0; i < 3; i++) {
                                c = parseInt(hex.substr(i*2,2), 16);
                                c = Math.round(Math.min(Math.max(0, c + (c * lum)), 255)).toString(16);
                                rgb += ("00"+c).substr(c.length);
                            }
//
                            return rgb;
                        }

                        var darker_color_hex = '#aa0000'; //ColorLuminance(color_hex, -0.1);

                        $('.next_slider').css({
                            'background-color' : darker_color_hex
                        });
                        $('.previous_slider').css({
                            'background-color' : darker_color_hex
                        });

                        $(ul).addClass('ul_margin');



                        $('.next_slider').click(function () {
                            var ul_offset_next = $(ul).offset();

                            if ((ul_offset_next.left + width_differences - ul_offset.left) < 100) {
                                $(ul).css({
                                    'left' : -1 * (width_differences) - 50
                                });
                            } else {
                                $(ul).css({
                                    'left': '-=100px'
                                });
                            }

                            var ul_css_left_temp = $(ul).css('left');

                        });

                        $('.previous_slider').click(function () {
                            $(ul).css({
                                'left': 0
                            });
                        });
                    }

                }, 500);
            }

            function contact_us_slider() {
                setTimeout(function(){
                    var ul = ".contact-us #block-campus-location-campuses-locations-content ul.nav-tabs";
                    var wrapper = "#block-campus-location-campuses-locations-content";
                    var wrapper_offset = $(wrapper).offset();

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $( ".contact-us #block-campus-location-campuses-locations-content ul.nav-tabs li").each(function(index) {
                        var current_width = parseInt($(this).width(), 10);
                        if (current_width > 0) {
                            totalWidth += current_width;
                        }
                    });

                    totalWidth += 50;


                    // ul offset //

                    var ul_offset = $(ul).offset();
                    //

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(wrapper).append('<div class="previous_slider"></div>');
                        $(wrapper).append('<div class="next_slider"></div>');
                        $('.next_slider').css({
                            'background-color' : '#AB0000'
                        });
                        $('.previous_slider').css({
                            'background-color' : '#AB0000'
                        });

                        $(ul).addClass('ul_margin');



                        $('.next_slider').click(function () {
                            var ul_offset_next = $(ul).offset();

                            if ((ul_offset_next.left + width_differences - ul_offset.left) < 150) {
                                $(ul).css({
                                    'left' : -1 * (width_differences) - 50
                                });
                            } else {
                                $(ul).css({
                                    'left': '-=150px'
                                });
                            }

                            var ul_css_left_temp = $(ul).css('left');

                        });

                        $('.previous_slider').click(function () {
                            $(ul).css({
                                'left': 0
                            });
                        });
                    } else {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(ul).removeClass('ul_margin');
                        $(ul).css({
                            'left' : '0'
                        });
                    }

                }, 500);
            }

            function news_and_events_slider() {
                setTimeout(function(){
                    var ul = ".page-media-center .main-container section ul.tabs--primary";
                    var wrapper = ".faculty-profiles-wrapper";
                    $(wrapper).css({
                        'padding-bottom' : '7px'
                    });

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $(".page-media-center .main-container section ul.tabs--primary li").each(function(index) {
                        var current_width = parseInt($(this).width(), 10);
                        if (current_width > 0) {
                            totalWidth += current_width;
                        }
                    });


                    totalWidth += 50;

                    // ul offset //

                    var ul_offset = $(ul).offset();

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(wrapper).append('<div class="previous_slider"></div>');
                        $(wrapper).append('<div class="next_slider"></div>');

                        var color = 'rgb(191, 0, 0)';
                        function rgb2hex(rgb){
                            rgb = rgb.match(/^rgba?[\s+]?\([\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?/i);
                            return (rgb && rgb.length === 4) ? "#" +
                            ("0" + parseInt(rgb[1],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[2],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[3],10).toString(16)).slice(-2) : '';
                        }

                        var color_hex = rgb2hex(color);

                        function ColorLuminance(hex, lum) {
//
                            // validate hex string
                            hex = String(hex).replace(/[^0-9a-f]/gi, '');
                            if (hex.length < 6) {
                                hex = hex[0]+hex[0]+hex[1]+hex[1]+hex[2]+hex[2];
                            }
                            lum = lum || 0;
//
                            // convert to decimal and change luminosity
                            var rgb = "#", c, i;
                            for (i = 0; i < 3; i++) {
                                c = parseInt(hex.substr(i*2,2), 16);
                                c = Math.round(Math.min(Math.max(0, c + (c * lum)), 255)).toString(16);
                                rgb += ("00"+c).substr(c.length);
                            }
//
                            return rgb;
                        }

                        var darker_color_hex = ColorLuminance(color_hex, -0.1);

                        $('.next_slider').css({
                            'background-color' : darker_color_hex
                        });
                        $('.previous_slider').css({
                            'background-color' : darker_color_hex
                        });

                        $(ul).addClass('ul_margin');



                        $('.next_slider').click(function () {
                            var ul_offset_next = $(ul).offset();

                            if ((ul_offset_next.left + width_differences - ul_offset.left) < 150) {
                                $(ul).css({
                                    'left' : -1 * (width_differences) - 50
                                })
                            } else {
                                $(ul).css({
                                    'left': '-=150px'
                                });
                            }

                            var ul_css_left_temp = $(ul).css('left');
                            var ul_css_left = ul_css_left_temp.substr(0, ul_css_left_temp.length-2);
                        });

                        $('.previous_slider').click(function () {
                            $(ul).css({
                                'left': 0
                            });
                        });
                    }  else {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(ul).removeClass('ul_margin');
                        $(ul).css({
                            'left' : '0'
                        });
                    }

                }, 500);
            }

            function calendar_slider() {
                setTimeout(function(){
                    var ul = ".page-event-created  .main-container section ul.tabs--primary";
                    var wrapper = ".faculty-profiles-wrapper";
                    $(wrapper).css({
                        'padding-bottom' : '7px'
                    });

                    var width = $(wrapper).width();

                    var totalWidth = 0;
                    $(".page-event-created  .main-container section ul.tabs--primary li").each(function(index) {
                        var current_width = parseInt($(this).width(), 10);
                        if (current_width > 0) {
                            totalWidth += current_width;
                        }
                    });


                    totalWidth += 50;

                    // ul offset //

                    var ul_offset = $(ul).offset();

                    var width_differences = totalWidth - width;

                    if (width_differences > 0) {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(wrapper).append('<div class="previous_slider"></div>');
                        $(wrapper).append('<div class="next_slider"></div>');

                        var color = 'rgb(191, 0, 0)';
                        function rgb2hex(rgb){
                            rgb = rgb.match(/^rgba?[\s+]?\([\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?,[\s+]?(\d+)[\s+]?/i);
                            return (rgb && rgb.length === 4) ? "#" +
                            ("0" + parseInt(rgb[1],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[2],10).toString(16)).slice(-2) +
                            ("0" + parseInt(rgb[3],10).toString(16)).slice(-2) : '';
                        }

                        var color_hex = rgb2hex(color);

                        function ColorLuminance(hex, lum) {
//
                            // validate hex string
                            hex = String(hex).replace(/[^0-9a-f]/gi, '');
                            if (hex.length < 6) {
                                hex = hex[0]+hex[0]+hex[1]+hex[1]+hex[2]+hex[2];
                            }
                            lum = lum || 0;
//
                            // convert to decimal and change luminosity
                            var rgb = "#", c, i;
                            for (i = 0; i < 3; i++) {
                                c = parseInt(hex.substr(i*2,2), 16);
                                c = Math.round(Math.min(Math.max(0, c + (c * lum)), 255)).toString(16);
                                rgb += ("00"+c).substr(c.length);
                            }
//
                            return rgb;
                        }

                        var darker_color_hex = ColorLuminance(color_hex, -0.1);

                        $('.next_slider').css({
                            'background-color' : darker_color_hex
                        });
                        $('.previous_slider').css({
                            'background-color' : darker_color_hex
                        });

                        $(ul).addClass('ul_margin');



                        $('.next_slider').click(function () {
                            var ul_offset_next = $(ul).offset();

                            if ((ul_offset_next.left + width_differences - ul_offset.left) < 150) {
                                $(ul).css({
                                    'left' : -1 * (width_differences) - 50
                                })
                            } else {
                                $(ul).css({
                                    'left': '-=150px'
                                });
                            }

                            var ul_css_left_temp = $(ul).css('left');
                            var ul_css_left = ul_css_left_temp.substr(0, ul_css_left_temp.length-2);
                        });

                        $('.previous_slider').click(function () {
                            $(ul).css({
                                'left': 0
                            });
                        });
                    }  else {
                        $('.previous_slider').remove();
                        $('.next_slider').remove();
                        $(ul).removeClass('ul_margin');
                        $(ul).css({
                            'left' : '0'
                        });
                    }

                }, 500);
            }

