            if($('.selectricItems').length > 0 ) {
                $('#views-exposed-form-open-enrolment-programmes-page .views-submit-button').remove();
                $('#ceibs-programmes-filters-form button').remove();

                $('.page-open-enrolment-programmes form .selectricItems li').click(function () {
                    $('#views-exposed-form-open-enrolment-programmes-page').submit();
                });

                $('.page-find-your-programme form .selectricItems li').click(function () {
                    $('#ceibs-programmes-filters-form').submit();
                });

                $('.alumni form .selectricItems li').click(function(){
                    if($(this).text() !== "Select") {
                        $('#-archive-select-form').submit();
                    }
                });
            }
            $('.page-open-enrolment-programmes form select option').click(function(){
                $('#views-exposed-form-open-enrolment-programmes-page').submit();
                alert('test1');
            });

            $('.page-find-your-programme form select option').click(function(){
                $('#ceibs-programmes-filters-form').submit();
