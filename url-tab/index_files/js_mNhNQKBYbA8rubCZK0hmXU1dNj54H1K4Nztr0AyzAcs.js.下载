                              };
            $(category_li).each(function(i)
            {
                if($(this).text() == "Category") {                          $(this).text("All"); }
                if($(this).text() == "All") {                               $(this).addClass('all-icon'); }
                if($.inArray($.trim($(this).text()), strings.c_suite_series) >= 0) {  $(this).addClass('c-suite-series-icon'); }
                if($.inArray($.trim($(this).text()), strings.corporate_governance) >= 0) {              $(this).addClass('corporate-governance-icon'); }
                if($.inArray($.trim($(this).text()), strings.finance_and_accounting) >= 0) {            $(this).addClass('finance-icon'); }
                if($.inArray($.trim($(this).text()), strings.general_management) >= 0) {                $(this).addClass('general-management-icon'); }
				if($.inArray($.trim($(this).text()), strings.family_business)>=0) {                     $(this).addClass('family-business-icon'); }
                if($.inArray($.trim($(this).text()), strings.leadership)>=0) {                        $(this).addClass('leadership-icon'); }
                if($(this).text() == "Multinational Management") {          $(this).addClass('multinational-management-icon'); }
                if($.inArray($.trim($(this).text()), strings.strategy)>=0) {                          $(this).addClass('strategy-icon'); }
                if($.inArray($.trim($(this).text()), strings.innovation)>=0) {                        $(this).addClass('innovation-icon'); }
                if($.inArray($.trim($(this).text()), strings.marketing_and_sales)>=0) {               $(this).addClass('marketing-and-sales-icon'); }
                if($.inArray($.trim($(this).text()), strings.negotiation_and_decision_making) >= 0) {   $(this).addClass('negotiation-and-decision-making-icon'); }
                if($.inArray($.trim($(this).text()), strings.operations_management)>=0) {             $(this).addClass('operations-icon'); }
                if($.inArray($.trim($(this).text()), strings.organisation_and_management)>=0) {       $(this).addClass('organisation-and-management-icon'); }
                if($.inArray($.trim($(this).text()), strings.others)>= 0) {                            $(this).addClass('others-icon'); }
                if($.inArray($.trim($(this).text()), strings.entrepreneurship) >= 0) {                  $(this).addClass('entrepreneurship-icon'); }
                if($.inArray($.trim($(this).text()), strings.study_tour) >= 0) {                  $(this).addClass('study-tour-icon'); }
				if($.inArray($.trim($(this).text()), strings.to_be_alumni) >= 0) {                  $(this).addClass('alumni-icon'); }
                if($.inArray($.trim($(this).text()), strings.industry_programmes) >= 0) {                  $(this).addClass('industry-programmes-icon'); }
            });

